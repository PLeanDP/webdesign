<script type="text/javascript"> 
 $(function() {
        var offset = $("#partners_bigger_bridge").offset();  
        $(window).scroll(function() {
            if ($(window).scrollTop() > offset.top + 460) {
                 $("#partners_bigger_bridge").addClass('partners_bridge_fixed');
            } else {
                 $("#partners_bigger_bridge").removeClass('partners_bridge_fixed');
            };
        });
    });  
</script>





